<% if images.size > 0 %>
<ul style="padding: 10px; list-style: none;">
  <% images.each do |image| %>
    <li>
      <a href="javascript:void(0)" onclick="<%="ts_insert_image('#{image.public_filename()}', '#{image.filename}');" %>">
      <%= image_tag( image.public_filename(:thumb) ) %></a>
    </li>
  <% end %>
</ul>
<% else %>
 No Images Uploaded Yet.
<% end %>