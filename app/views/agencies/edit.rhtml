  <p>
    <b>Has geo restrictions</b><br />
    <%= f.text_field :has_geo_restrictions %>
  </p>

  <p>
    <b>Agency category</b><br />
    <%= f.text_field :agency_category_id %>
  </p>

  <p>
    <b>Agency type</b><br />
    <%= f.text_field :agency_type_id %>
  </p>

  <p>
    <b>Created at</b><br />
    <%= f.text_field :created_at %>
  </p>

  <p>
    <b>Updated at</b><br />
    <%= f.text_field :updated_at %>
  </p>

  <p>
    <b>Updated by</b><br />
    <%= f.text_field :updated_by %>
  </p>

  <p>
    <%= submit_tag "Update" %>
  </p>
