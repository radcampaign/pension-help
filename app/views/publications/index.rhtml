<h1>Listing publications</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Phone ext</th>
    <th>Tollfree</th>
    <th>Tollfree ext</th>
    <th>Fax</th>
    <th>Tty</th>
    <th>Tty ext</th>
    <th>Email</th>
    <th>Url</th>
    <th>Url title</th>
  </tr>
  
<% for publication in @publications %>
  <tr>
    <td><%=h publication.name %></td>
    <td><%=h publication.phone %></td>
    <td><%=h publication.phone_ext %></td>
    <td><%=h publication.tollfree %></td>
    <td><%=h publication.tollfree_ext %></td>
    <td><%=h publication.fax %></td>
    <td><%=h publication.tty %></td>
    <td><%=h publication.tty_ext %></td>
    <td><%=h publication.email %></td>
    <td><%=h publication.url %></td>
    <td><%=h publication.url_title %></td>
    <td><%= link_to 'Show', publication_path(publication) %></td>
    <td><%= link_to 'Edit', edit_publication_path(publication) %></td>
    <td><%= link_to 'Destroy', publication_path(publication), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New publication', new_publication_path %>